import turtle

t = turtle.Turtle()



t.home()

t.clear()



for x in range(4):

  t.forward(100)

  t.left(90)



print('hello')




t.home()

t.clear()



for i in range(4):

  t.up()

  t.forward(20)

  t.down()

  for x in range(4):

    t.forward(100)

    t.left(90)

  t.left(-90)



print('hello')



t.home()

t.clear()



for i in range(4):

  t.up()

  t.forward(20)

  t.down()

  for x in range(4):

    t.forward(100)

    t.left(90)

  t.left(-90)



print('hello')



def circle(x, y, r = 150):

  t.up()

  t.home()

  t.forward(x)

  t.left(90)

  t.forward(y)

  t.forward(r)

  t.left(90)

  t.down()

  t.circle(r)



t.home()

t.clear()



circle(100, -100)

circle(100, 100, 50)

circle(-100, -100, 50)

circle(-100, 100, 50)








































		